{
  "name": "lacona-dnd",
  "version": "1.0.0",
  "description": "Lacona Addon to look up D&D 5E spells, etc.",
  "main": "lib/index.js",
  "lacona": {
    "title": "D&D Helper",
    "description": "Look up spells, etc.",
    "config": "config.json"
  },
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "prepare": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/dhleong/lacona-dnd"
  },
  "keywords": [
    "lacona",
    "lacona-addon"
  ],
  "license": "MIT",
  "dependencies": {
    "elliptical": "^1.0.0",
    "lacona-api": "^1.0.0",
    "lacona-phrases": "^1.0.0",
    "mkdirp": "^0.5.1",
    "util.promisify": "^1.0.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-cli": "^6.0.0",
    "rimraf": "^2.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-react-jsx"
    ]
  }
}
